    "babel-eslint": "^10.0.1",
    "babel-jest": "^27.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "7.0.3",
    "css-loader": "6.2.0",
    "cypress": "^8.3.1",
    "eslint": "^7.32.0",
