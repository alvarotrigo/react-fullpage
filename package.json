{
  "name": "@fullpage/react-fullpage",
  "version": "0.1.45",
  "description": "Official react wrapper for fullPage.js",
  "author": "cmswalker",
  "contributors": [
    "cmswalker",
    "alvarotrigo"
  ],
  "main": "./dist/react-fullpage.js",
