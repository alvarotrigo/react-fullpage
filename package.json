    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.3.2",
    "jest": "^27.1.0",
    "jest-dom": "^3.0.1",
    "kcd-scripts": "^11.2.0",
    "node-sass": "6.0.1",
    "prettier": "^2.3.2",
    "prettier-eslint": "^13.0.0",
    "react": "^17.0.2",
